<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>TheStore</display-name>
	<servlet>
		<servlet-name>dingdanServlet</servlet-name>
		<servlet-class>com.servlet.dingdanServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PhonesServlet</servlet-name>
		<servlet-class>com.servlet.PhonesServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserProductServlet</servlet-name>
		<servlet-class>com.servlet.UserProductServlet</servlet-class>
	</servlet>
  <servlet>
    <servlet-name>DingdanServlet</servlet-name>
    <servlet-class>com.servlet.DingdanServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PayServlet</servlet-name>
    <servlet-class>com.servlet.PayServlet</servlet-class>
  </servlet>
	<session-config>
	<session-timeout>14400</session-timeout>
	</session-config>

	<servlet-mapping>
		<servlet-name>dingdanServlet</servlet-name>
		<url-pattern>/dingdanServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PhonesServlet</servlet-name>
		<url-pattern>/PhonesServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserProductServlet</servlet-name>
		<url-pattern>/UserProductServlet</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>DingdanServlet</servlet-name>
    <url-pattern>/DingdanServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PayServlet</servlet-name>
    <url-pattern>/PayServlet</url-pattern>
  </servlet-mapping>
	<servlet>
		<servlet-name>CategoryServlet</servlet-name>
		<servlet-class>com.servlet.CategoryServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>JDDispatcherServlet</servlet-name>
		<servlet-class>com.servlet.JDDispatcherServlet</servlet-class>
	</servlet>
	<servlet>

		<servlet-name>Category2Servlet</servlet-name>
		<servlet-class>com.servlet.Category2Servlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ProductServlet</servlet-name>
		<servlet-class>com.servlet.ProductServlet</servlet-class>
		<init-param>
			<param-name>productImageUploadPath</param-name>
			<param-value>D:/WorkSpace/TheStore/WebRoot/img/productImage</param-value>
		</init-param>
	</servlet>

	<servlet>
		<servlet-name>ShoppingCarServlet</servlet-name>
		<servlet-class>com.servlet.ShoppingCarServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.servlet.UserServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>OrderServlet</servlet-name>
		<servlet-class>com.servlet.OrderServlet</servlet-class>
	</servlet>
	<servlet>
    <servlet-name>Servlet</servlet-name>
    <servlet-class>com.servlet.Servlet</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>UsernameServlet</servlet-name>
    <servlet-class>com.servlet.UsernameServlet</servlet-class>
  </servlet>
     <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
 
 
  <servlet-mapping>
    <servlet-name>UsernameServlet</servlet-name>
    <url-pattern>/UsernameServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>com.servlet.AdminServlet</servlet-class>
  </servlet>

  <servlet-mapping>   
    <servlet-name>Servlet</servlet-name>
    <url-pattern>/Servlet</url-pattern>
  </servlet-mapping>
	

	<servlet-mapping>
		<servlet-name>CategoryServlet</servlet-name>
		<url-pattern>/CategoryServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>JDDispatcherServlet</servlet-name>
		<url-pattern>/JDDispatcherServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Category2Servlet</servlet-name>
		<url-pattern>/Category2Servlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProductServlet</servlet-name>
		<url-pattern>/ProductServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ShoppingCarServlet</servlet-name>
		<url-pattern>/ShoppingCarServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/UserServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>OrderServlet</servlet-name>
		<url-pattern>/OrderServlet</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/AdminServlet</url-pattern>
  </servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	<filter>
		<filter-name>MyCharacterFilter</filter-name>
		<filter-class>com.filter.MyCharacterFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>MyCharacterFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>


